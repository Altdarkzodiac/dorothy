#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# https://github.com/dalance/procs
# ps alternative

function setup-util-procs() (
	setup-util "$@" CLI='procs' \
		APK='procs' \
		BREW='procs' \
		CARGO='procs' \
		DNF='procs' \
		NIX='procs' \
		PACMAN='procs' \
		SCOOP='procs' \
		SNAP='procs'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-procs "$@"
fi
