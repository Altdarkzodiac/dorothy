#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

function setup-install() (
	# pre-requisites
	is-internet-working

	# carry on
	if is-mac; then
		setup-mac install
	elif is-linux; then
		setup-linux install
	fi
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-install "$@"
fi
