#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# https://github.com/bootandy/dust
# du alternative

function setup-util-dust() (
	setup-util "$@" CLI='dust' \
		BREW='dust' \
		CARGO='du-dust'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-dust "$@"
fi
