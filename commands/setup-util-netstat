#!/usr/bin/env bash

# also provides [ip] command

function setup-util-netstat() (
	source "$DOROTHY/sources/strict.bash"

	setup-util "$@" --cli=netstat \
		APK='net-tools' \
		APT='net-tools' \
		AUR='net-tools'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-netstat "$@"
fi
