#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# https://github.com/pemistahl/grex
# regexp builder

function setup-util-grex() (
	setup-util "$@" CLI='grex' \
		BREW='grex' \
		CARGO='grex' \
		CHOCO='grex' \
		HUBER='grex' \
		SCOOP='grex'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-grex "$@"
fi
