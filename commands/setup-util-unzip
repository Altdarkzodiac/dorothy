#!/usr/bin/env bash

function setup-util-unzip() (
	source "$DOROTHY/sources/strict.bash"

	setup-util "$@" --cli='unzip' \
		APK='unzip' \
		APT='unzip' \
		AUR='unzip' \
		BREW='unzip' \
		RPM='unzip' \
		ZYPPER='unzip'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-unzip "$@"
fi
