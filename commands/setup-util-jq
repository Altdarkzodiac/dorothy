#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

function setup-util-jq() (
	setup-util "$@" CLI='jq' \
		APT='jq' \
		BREW='jq' \
		CHOCO='jq' \
		DNF='jq' \
		PACMAN='jq' \
		PKG='jq' \
		PKGUTIL='jq' \
		PORT='jq' \
		ZYPPER='jq'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-jq "$@"
fi
