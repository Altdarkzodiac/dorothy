#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# https://github.com/aria2/aria2
# https://github.com/aria2/aria2/releases
# wget alternative

function setup-util-aria2() (
	setup-util "$@" --cli='aria2c' \
		BREW='aria2' \
		APK='aria2' \
		APT='aria2' \
		PACMAN='aria2'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-aria2 "$@"
fi
