	BREW='glab' \
	WINGET='glab' \
	SCOOP='glab' \
	SNAP='glab' SNAP_CHANNEL='edge' \
	YAY='gitlab-glab-bin' \
	KISS='gitlab-glab' \
	APK='glab' \
	NIX='nixos.glab' \
	PORT='glab' \
	DOWNLOAD='https://j.mp/glab-cli' DOWNLOAD_FILENAME='glab-cli'
