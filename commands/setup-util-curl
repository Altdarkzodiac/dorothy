#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

function setup-util-curl() (
	setup-util "$@" --cli='curl' \
		APK='curl' \
		APT='curl' \
		AUR='curl' \
		BREW='curl' \
		RPM='curl' \
		ZYPPER='curl'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-curl "$@"
fi
