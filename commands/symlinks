#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# =====================================
# Arguments

# help
if is-help-empty "$@"; then
	cat <<-EOF >/dev/stderr
	    ABOUT:
	        List only symlinks.
	EOF
	if test "$#" -ne 0; then
		echo-style $'\n' --error="ERROR:" $'\n' --red="$(echo-lines -- "$@")" >/dev/stderr
	fi
	return 22 # Invalid argument
fi

# =====================================
# Act

# shellcheck disable=SC2010
ls -la | grep ^l
