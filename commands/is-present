#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

# you should use `test -v 'name'`` instead of this
# note that `test -v` is bash v4+ only

# trunk-ignore(shellcheck/SC2235)
function is-present() (
	test -n "${1-}"
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	is-present "$@"
fi
