#!/usr/bin/env sh

# help
if is-help "$@"; then
	cat <<-EOF >/dev/stderr
		ABOUT:
		??????
	EOF
	# if test "$#" -ne 0; then
	# 	echo-style $'\n' --error="ERROR:" $'\n' --red="$(echo-lines -- "$@")" >/dev/stderr
	# fi
	return 22 # Invalid argument
fi

if test -n "$FISH_VERSION"; then
	# shellcheck disable=3045
	type --quiet "$1"
else
	silent type "$1"
fi
