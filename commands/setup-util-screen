#!/usr/bin/env bash
source "$DOROTHY/sources/strict.bash"

function setup-util-screen() (
	setup-util "$@" --cli='screen' \
		APK='screen' \
		APT='screen' \
		BREW='screen' \
		RPM='screen' \
		ZYPPER='screen'
)

# fire if invoked standalone
if test "$0" = "${BASH_SOURCE[0]}"; then
	setup-util-screen "$@"
fi
