#!/usr/bin/env bash
set -e

# Setup shell configuration
if is_string "$ZSH_VERSION"; then
	echo 'zsh'
elif is_string "$FISH_VERSION"; then
	echo 'fish'
elif is_string "$BASH_VERSION"; then
	echo 'bash'
elif is_string "$KSH_VERSION"; then
	echo 'ksh'
elif is_string "$FCEDIT"; then
	echo 'ksh'
elif is_string "$PS3"; then
	echo 'unknown'
else
	echo 'sh'
fi