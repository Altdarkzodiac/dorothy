#!/usr/bin/env bash
set -e

echo -e "\nInstalling commenced..."

setup-mac-settings
setup-mac-brew
setup-mac-apps
setup-mac-fonts

setup-bin
setup-git
setup-node
together \
	setup-ruby \
	setup-python \
	setup-vscode \
	setup-atom \
	;
setup-bin

setup-mac-clean

setup-shell

echo -e "\nInstallation completed successfully."